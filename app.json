{
  "expo": {
    "name": "Melo-MP",
    "slug": "Melo-MP",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "melomp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "icon": "./assets/images/icon.png",

    "ios": {
      "supportsTablet": true
    },

    "android": {
      "package": "com.bhoffman4.MeloMP",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png",
        "backgroundColor": "#E6F4FE"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },

    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },

    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/splash-icon.png",
          "resizeMode": "contain",
          "backgroundColor": "#0F172A",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-secure-store",
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "merchant.com.melo.marketplace"
        }
      ]
    ],

    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },

    "extra": {
    
      "stripePublishableKey": "pk_test_51SpqdDDrYUL6FG4RCzhN6GkOapTn3rscgruuDDHeSJPfLFwY8LlSDNtZhXRrXXEkZKjj1GJs4GOrtILeSCcMd0OR00Bx8vmJCf",

      "router": {},
      "eas": {
        "projectId": "d3c5c61c-a428-41f5-b2cd-ce9bc35b2f3c"
      }
    },

    "owner": "bhoffman4"
  }
}
